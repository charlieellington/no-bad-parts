# Environment variables for Waitlist Kit
# Rename this file to .env.local (Next.js) or add the values in your Vercel dashboard.

NEXT_PUBLIC_SUPABASE_URL=
NEXT_PUBLIC_SUPABASE_ANON_KEY=

# Optional – elevated server key (needed only if you enable server-side inserts outside RLS scope)
# SUPABASE_SERVICE_ROLE_KEY= 

# — Re-Send (email provider)
RESEND_API_KEY=
RESEND_FROM_EMAIL=no-reply@resend.dev  # replace once your domain is verified 

# Control whether transactional welcome e-mails are sent after sign-up
ENABLE_WELCOME_EMAIL=false 